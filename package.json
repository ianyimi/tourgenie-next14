  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "format": "prettier --write ./src/**/*.{ts,tsx}",
    "db:generate": "dotenv drizzle-kit generate:pg --config drizzle.config.ts",
    "db:pull": "dotenv drizzle-kit introspect:pg --config drizzle.config.ts",
