    "db:check": "dotenv drizzle-kit check:pg --config drizzle.config.ts",
    "db:drop": "dotenv drizzle-kit drop --config drizzle.config.ts",
    "db:studio": "dotenv drizzle-kit studio --host 127.0.0.1 --port 3005",
    "db:migrate": "tsx src/server/db/migrate"
  },
  "dependencies": {
