    "@trpc/react-query": "^10.37.1",
    "@trpc/server": "^10.37.1",
    "drizzle-orm": "^0.28.5",
    "drizzle-zod": "^0.5.1",
    "next": "^13.5.4",
    "pg": "^8.11.3",
    "postgres": "^3.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-map-gl": "^7.1.6",
    "superjson": "^1.13.1",
